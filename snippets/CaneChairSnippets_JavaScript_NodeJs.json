{
	"NodeJs:Common:Export:*": {
		"prefix": "ccNodeJs_Common_Export_*",
		"body": [
			"export * from '${1:__modulePath__}';"
		],
		"description": "export * from '${1:__modulePath__}';"
	},
	"NodeJs:Function:For": {
		"prefix": "ccNodeJs_Function_For",
		"body": [
			"for (let ${1:__entityname__} in ${2:__modelName__}) {",
			"\t${3:__statement__}",
			"}"
		],
		"description": "example: if(condition) { statement }"
	},
	"NodeJs:Require": {
		"prefix": "ccNodeJs_Require",
		"body": [
			"${2:__moduleName__}=require('${1:__modulePath__}');"
		],
		"description": ""
	},
	"NodeJs:Function:NewFunction": {
		"prefix": "ccNodeJs_Function_NewFunction",
		"body": [
			"function ${1:__functionName__}(${2:__parameter__}){",
			"\t${3:__statement__}",
			"};"
		],
		"description": ""
	}
}