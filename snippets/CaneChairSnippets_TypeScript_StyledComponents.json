    "prefix": "ccStyledComponent_${1:__ComponentName__}",
    "body": [
      "export const ${1:__ComponentName__} = styled.div`",
      " align-items: flex-start;",
      " background-color: #3179ba;",
      " display: flex;",
      " flex-direction: row;",
      " height: 100%;",
      " padding: 20px;",
      " width: 100%;",
      "`;"
    ]
  },
  "StyledComponent : Button": {
    "prefix": "ccStyledComponent_Button",
    "body": [
      "export const ${1:__ComponentName__} = styled.button<${1:__ComponentName__}Props>`:",
      " ${0}",
      "`;"
    ]
  }
}
