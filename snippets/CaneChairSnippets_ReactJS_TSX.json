    "ReactJS : TSX : Component : ListComponent": {
        "prefix": "ccReactJS_TSX_Component_ListComponent",
        "body": [
            "import * as React from 'react';",
            "import { ${1:__ComponentName__}Model } from 'types/modelTypes/${1:__ComponentName__}Model';",
            "import ReactTable from 'react-table';",
            "import 'react-table/react-table.css';",
            "import { Link } from 'react-router-dom';",
            "",
            "const ${1:__ComponentName__}List = (props: ${1:__ComponentName__}ListProps) => {",
            "  const columns = [",
            "    {",
            "      Header: 'Id',",
            "      accessor: 'id'",
            "    },",
            "    {",
            "      Header: 'Name',",
            "      accessor: '${1:__ComponentName__}Name',",
            "      Cell: (d: any) => <span className=\"${1:__ComponentName__}Name\">{d.value}</span>",
            "    },",
            "    {",
            "      Header: '',",
            "      id: 'btn_details',",
            "      Cell: (d: any) => <Link to={`/${1:__ComponentName__}/${d.original.id}`} className=\"btn btn-outline-success btn-sm mx-1\" > Details</Link>,",
            "      filterable: false,",
            "      maxWidth: 75",
            "    },",
            "    {",
            "      Header: '',",
            "      id: 'btn_delete',",
            "      Cell: (d: any) => <input type=\"button\" className=\"btn btn-outline-danger btn-sm mx-1\" onClick={() => { props.on${1:__ComponentName__}Delete(d.original); }} value=\"Delete\" />,",
            "      filterable: false,",
            "      maxWidth: 75",
            "    }",
            "  ];",
            "",
            "  return (",
            "",
            "    <div>",
            "      {/* List of ${1:__ComponentName__}s */}",
            "      <ReactTable",
            "        data={props.${1:__ComponentName__}Array}",
            "        columns={columns}",
            "        defaultPageSize={10}",
            "        minRows={3}",
            "        filterable",
            "        defaultFilterMethod={(filter, row) => String(row[filter.id]).includes(filter.value)}",
            "      />",
            "    </div>",
            "  );",
            "};",
            "",
            "type ${1:__ComponentName__}ListProps = {",
            "  ${1:__ComponentName__}Array: ${1:__ComponentName__}Model[];",
            "  on${1:__ComponentName__}Delete: (${1:__ComponentName__}: ${1:__ComponentName__}Model) => void;",
            "};",
            "",
            "export default ${1:__ComponentName__}List;",
            ""
        ],
        "description": "Create a list component (TSX)"
    },
    "ReactJS : TSX : Component : DetailComponent": {
        "prefix": "ccReactJS_TSX_Component_DetailComponent",
        "body": [
            "import * as React from 'react';",
            "import { connect } from 'react-redux';",
            "import { bindActionCreators, Dispatch } from 'redux';",
            "import { withRouter, RouteComponentProps } from 'react-router-dom';",
            "import Store from 'store/store';",
            "import { ${1:__ModelName__}Model } from 'types/modelTypes/${1:__ModelName__}Model';",
            "import I${1:__ModelName__}Action from 'actions/interfaces/I${1:__ModelName__}Action';",
            "import * as ${1:__ModelName__}Actions from 'actions/${1:__ModelName__}Actions';",
            "import StoreStateType from 'types/StateTypes/StoreStateType';",
            "import CCTextField from 'components/CommonComponent/CCTextField';",
            "import * as toastr from 'toastr';",
            "const UUID = require('uuid/v4');",
            "",
            "class ${1:__ModelName__}Detail extends React.Component<ThisPropsType, ThisStateType> {",
            "  constructor(props: StateToPropsType) {",
            "    super(props as any);",
            "    this.state = {",
            "      ${1:__ModelName__}Array: this.props.${1:__ModelName__}Array,",
            "      current${1:__ModelName__}: this.props.current${1:__ModelName__},",
            "      errors: [],",
            "      isNew${1:__ModelName__}: this.props.isNew${1:__ModelName__},",
            "      isFormSaving: false,",
            "      textFields: {",
            "        id: (this.props.current${1:__ModelName__}.id !== undefined) ? this.props.current${1:__ModelName__}.id : '',",
            "        ${1:__ModelName__}Name: (this.props.current${1:__ModelName__}.${1:__ModelName__}Name !== undefined) ? this.props.current${1:__ModelName__}.${1:__ModelName__}Name : '',",
            "        ${1:__ModelName__}Description: (this.props.current${1:__ModelName__}.${1:__ModelName__}Description !== undefined) ? this.props.current${1:__ModelName__}.${1:__ModelName__}Description : ''",
            "      },",
            "      textFieldsErrors: {}",
            "    };",
            "  }",
            "",
            "  render() {",
            "    return (",
            "      <div className=\"container mt-3\">",
            "        <form onSubmit={this.onFormSubmit}>",
            "          <div id=\"\" className=\"\">",
            "            <input type=\"button\" className=\"btn btn-outline-warning btn-sm\" onClick={() => { this.props.history.push('/${1:__ModelName__}s'); }} value={'<= Go Back'} />",
            "            <h4 className=\"my-3\">${1:__ModelName__}</h4>",
            "          </div>",
            "          <div className=\"\">",
            "            <div className=\"form-row\">",
            "              <div className=\"col\">",
            "                <CCTextField",
            "                  fieldName=\"id\"",
            "                  label=\"${1:__ModelName__} ID\"",
            "                  value={this.state.textFields.id}",
            "                  onChange={this.onTextFieldChange}",
            "                />",
            "              </div>",
            "              <div className=\"col\">",
            "                <CCTextField",
            "                  fieldName=\"${1:__ModelName__}Name\"",
            "                  label=\"${1:__ModelName__} Name\"",
            "                  value={this.state.textFields.${1:__ModelName__}Name}",
            "                  isRequired={true}",
            "                  isRequiredErrorMessage=\"${1:__ModelName__} Name is required....\"",
            "                  onChange={this.onTextFieldChange}",
            "                  validate={(value) => value.length > 2 ? false : '${1:__ModelName__} name cannot less than 3 letters.'}",
            "                />",
            "              </div>",
            "              <div className=\"col\">",
            "                <CCTextField",
            "                  fieldName=\"${1:__ModelName__}Description\"",
            "                  label=\"Description\"",
            "                  value={this.state.textFields.${1:__ModelName__}Description}",
            "                  isRequired={false}",
            "                  isRequiredErrorMessage=\"\"",
            "                  onChange={this.onTextFieldChange}",
            "                  validate={(value) => value.length < 300 ? false : 'Description should less than 300 characters.'}",
            "                />",
            "              </div>",
            "              <input",
            "                type=\"submit\"",
            "                className=\"btn btn-outline-success form-control mx-1\"",
            "                value={this.state.isNew${1:__ModelName__} ? 'Add' : 'Update'}",
            "              />",
            "            </div>",
            "          </div>",
            "        </form>",
            "      </div>",
            "    );",
            "  }",
            "",
            "  componentWillMount() {",
            "    // Store.dispatch(${1:__ModelName__}Actions.getAll${1:__ModelName__}s());",
            "  }",
            "",
            "  componentDidMount() {",
            "    // if the component is loaded through browser url instead of <Link>, reload all data",
            "    // if (this.state.current${1:__ModelName__}.id === undefined) {",
            "    if (this.state.${1:__ModelName__}Array.length === 0) {",
            "      Store.dispatch(${1:__ModelName__}Actions.getAll${1:__ModelName__}s());",
            "    }",
            "  }",
            "",
            "  componentWillReceiveProps(nextProps: StateToPropsType) {",
            "    // // Set current selected ${1:__ModelName__} to state",
            "    if (nextProps.current${1:__ModelName__} !== undefined) {",
            "      this.setState({",
            "        current${1:__ModelName__}: Object.assign({}, nextProps.current${1:__ModelName__}),",
            "        textFields: {",
            "          id: nextProps.current${1:__ModelName__}.id,",
            "          ${1:__ModelName__}Name: nextProps.current${1:__ModelName__}.${1:__ModelName__}Name,",
            "          ${1:__ModelName__}Description: nextProps.current${1:__ModelName__}.${1:__ModelName__}Description",
            "        }",
            "      });",
            "    }",
            "    this.setState({ ${1:__ModelName__}Array: nextProps.${1:__ModelName__}Array });",
            "  }",
            "",
            "  componentDidUpdate() {",
            "    //",
            "",
            "  }",
            "",
            "  onTextFieldChange = (fieldName: string, value: string, error: string | boolean) => {",
            "    const textFields = this.state.textFields;",
            "    const textFieldsErrors = this.state.textFieldsErrors;",
            "",
            "    textFields[fieldName] = value;",
            "    textFieldsErrors[fieldName] = error;",
            "",
            "    this.setState({ textFields, textFieldsErrors });",
            "  }",
            "",
            "  onFormSubmit = (event: React.FormEvent<HTMLFormElement>) => {",
            "    event.preventDefault();",
            "    const ${1:__ModelName__}: ${1:__ModelName__}Model = new ${1:__ModelName__}Model();",
            "    ${1:__ModelName__}.id = this.state.textFields.id ? this.state.textFields.id : UUID();",
            "    ${1:__ModelName__}.${1:__ModelName__}Name = this.state.textFields.${1:__ModelName__}Name;",
            "    ${1:__ModelName__}.${1:__ModelName__}Description = this.state.textFields.${1:__ModelName__}Description;",
            "",
            "    if (!this.isFormValid()) {",
            "      return;",
            "    }",
            "",
            "    this.setState({ isFormSaving: true });",
            "",
            "    if (this.state.isNew${1:__ModelName__}) {",
            "      this.props.actions.addNew${1:__ModelName__}(${1:__ModelName__})",
            "        .then((res: any) => {",
            "          this.redirectTo${1:__ModelName__}sComponent();",
            "        })",
            "        .catch((error: string) => {",
            "          this.catchError(error);",
            "        });",
            "    } else {",
            "      this.props.actions.update${1:__ModelName__}(${1:__ModelName__})",
            "        .then(() => {",
            "          this.redirectTo${1:__ModelName__}sComponent();",
            "        })",
            "        .catch((error: string) => {",
            "          this.catchError(error);",
            "        });",
            "    }",
            "  }",
            "",
            "  redirectTo${1:__ModelName__}sComponent() {",
            "    this.setState({ isFormSaving: false });",
            "    toastr.success(this.state.isNew${1:__ModelName__} ? 'New ${1:__ModelName__} added.' : '${1:__ModelName__} updated.');",
            "    this.props.history.push('/${1:__ModelName__}s');",
            "  }",
            "",
            "  catchError(error: string) {",
            "    this.setState({ isFormSaving: false });",
            "    toastr.error(error);",
            "  }",
            "",
            "  isFormValid() {",
            "    const textFields = this.state.textFields;",
            "    const textFieldsErrors = this.state.textFieldsErrors;",
            "    const errorMessages = Object.keys(textFieldsErrors).filter((k) => textFieldsErrors[k]);",
            "",
            "    if (!textFields.${1:__ModelName__}Name) {",
            "      return false;",
            "    }",
            "",
            "    if (errorMessages.length) {",
            "      return false;",
            "    }",
            "",
            "    return true;",
            "  }",
            "}",
            "",
            "function get${1:__ModelName__}ById(${1:__ModelName__}s: ${1:__ModelName__}Model[], ${1:__ModelName__}Id: string) {",
            "  const ${1:__ModelName__} = ${1:__ModelName__}s.filter((s: ${1:__ModelName__}Model) => s.id === ${1:__ModelName__}Id);",
            "  if (${1:__ModelName__}) { return ${1:__ModelName__}[0]; }",
            "  return new ${1:__ModelName__}Model();",
            "}",
            "",
            "function mapStateToProps(storeState: StoreStateType, ownProps: OwnProps): StateToPropsType {",
            "  const ${1:__ModelName__}Id = ownProps.match.params.${1:__ModelName__}Id;",
            "  let current${1:__ModelName__} = new ${1:__ModelName__}Model();",
            "  let isNew${1:__ModelName__} = ${1:__ModelName__}Id === undefined;",
            "",
            "  if (${1:__ModelName__}Id && storeState.${1:__ModelName__}Array.length > 0) {",
            "    current${1:__ModelName__} = get${1:__ModelName__}ById(storeState.${1:__ModelName__}Array, ${1:__ModelName__}Id);",
            "    isNew${1:__ModelName__} = false;",
            "  }",
            "",
            "  return {",
            "    ${1:__ModelName__}Array: storeState.${1:__ModelName__}Array,",
            "    current${1:__ModelName__}: current${1:__ModelName__},",
            "    isNew${1:__ModelName__}: isNew${1:__ModelName__}",
            "  };",
            "}",
            "",
            "function mapDispatchToProps(dispatch: Dispatch<I${1:__ModelName__}Action>): DispatchToPropsType {",
            "  return {",
            "    actions: bindActionCreators(${1:__ModelName__}Actions, dispatch)",
            "  };",
            "}",
            "",
            "type ThisStateType = {",
            "  ${1:__ModelName__}Array: Array<${1:__ModelName__}Model>;",
            "  current${1:__ModelName__}: ${1:__ModelName__}Model;",
            "  errors: string[],",
            "  isNew${1:__ModelName__}: boolean,",
            "  isFormSaving: boolean;",
            "  textFields: {",
            "    id: string;",
            "    ${1:__ModelName__}Name: string;",
            "    ${1:__ModelName__}Description: string;",
            "  },",
            "  textFieldsErrors: {}",
            "};",
            "",
            "type StateToPropsType = {",
            "  ${1:__ModelName__}Array: Array<${1:__ModelName__}Model>;",
            "  current${1:__ModelName__}: ${1:__ModelName__}Model;",
            "  errors?: Object;",
            "  isNew${1:__ModelName__}: boolean;",
            "  isFormSaving?: boolean;",
            "};",
            "",
            "type DispatchToPropsType = {",
            "  actions: typeof ${1:__ModelName__}Actions;",
            "};",
            "",
            "type RCProps = RouteComponentProps<{ ${1:__ModelName__}Id: string }>;",
            "",
            "type OwnProps = {",
            "} & RCProps;",
            "",
            "type ThisPropsType = StateToPropsType & DispatchToPropsType & OwnProps;",
            "",
            "export default withRouter(connect<StateToPropsType, DispatchToPropsType, OwnProps>(mapStateToProps, mapDispatchToProps)(${1:__ModelName__}Detail));",
            ""
        ],
        "description": "Create a detail component (TSX)"
    }
}