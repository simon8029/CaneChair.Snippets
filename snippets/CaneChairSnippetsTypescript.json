{
	"Print to console": {
		"prefix": "cclog",
		"body": [
			"console.log(`$1`);"
		],
		"description": "Log output to console"
	},
	"Angular : Component/Service : Method : If": {
		"prefix": "ccAngular_Method_If",
		"body": [
			"if (${1:__condition__}) {",
			"\t${2:__statement__}",
			"}"
		],
		"description": "example: if(condition) { __statement__ }"
	},
	"Angular : Component/Service : Method : SwitchCase": {
		"prefix": "ccAngular_Method_Switch",
		"body": [
			"switch(${1:}) {",
			"\tcase ${2:__case__}:",
			"\t\t${3:__statement__} ",
			"\t\tbreak;",
			"\tdefault:",
			"\t\tbreak;",
			"}"
		],
		"description": "Switch Case"
	},
	"Angular : Component/Service : Interface": {
		"prefix": "ccAngular_Interface",
		"body": [
			"export interface I${1:__interfaceName__} {",
			"\t${2:__properties__}",
			"}"
		],
		"description": "example: export interface I${1:__interfaceName__}{}"
	},
	"Angular : Component/Service : Constructor": {
		"prefix": "ccAngular_ctor",
		"body": [
			"constructor(${1:__parameters__}) {${2}}"
		],
		"description": "constructor(${1:__parameters__}) {${2}}"
	},
	"Angular : Component/Service : Import : Module": {
		"prefix": "ccAngular_Import_Module",
		"body": [
			"import { ${2:moduleName} } from '${1:modulePath}';"
		],
		"description": "example: import { NgModule } from '@angular/core';"
	},
	"Angular : Component/Service : Import : As": {
		"prefix": "ccAngular_Import_As",
		"body": [
			"import * as ${2:__name__} from '${1:__path__}';"
		],
		"description": "import * as ${2:__name__} from '${1:__path__}';"
	},
	"Angular : Component/Service : Import : Redux": {
		"prefix": "ccAngular_Import_Redux",
		"body": [
			"import * as Redux from 'redux';"
		],
		"description": "example: import * as Redux from 'redux';"
	},
	"Angular : Component/Service : Export : Class": {
		"prefix": "ccAngular_Export_Class",
		"body": [
			"export class ${1:__className__} {",
			"\t${2:__statement__}",
			"}"
		],
		"description": "export class ${1:__className__} { }"
	},
	"Angular : Component/Service : Export : Function": {
		"prefix": "ccAngular_Export_Function",
		"body": [
			"export function ${1:__functionName__}(${2:__parameters__}) {",
			"\t${3:__statement__}",
			"}"
		],
		"description": "export function ${1:__functionName__} (${2:__parameters__}) { }"
	}
}