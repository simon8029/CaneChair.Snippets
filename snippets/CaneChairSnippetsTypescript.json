{
	"Print to console": {
		"prefix": "cclog",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	},
	"import {} from ''": {
		"prefix": "ccimps",
		"body": [
			"import { ${2:moduleName} } from \"${1:modulePath}\";"
		],
		"description": "example: import { NgModule } from \"@angular/core\";"
	},
	"if statement": {
		"prefix": "ccif",
		"body": [
			"if (${1:condition}) {",
			"${2:statement}",
			"}"
		],
		"description": "example: if(condition) { statement }"
	}
}