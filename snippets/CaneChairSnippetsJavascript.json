{
	"Print to console": {
		"prefix": "cclog",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	},
	"if statement": {
		"prefix": "ccif",
		"body": [
			"if (${1:condition}) {",
			"${2:statement}",
			"}"
		],
		"description": "example: if(condition) { statement }"
	},
	"anonymous function": {
		"prefix": "ccaf",
		"body": [
			"(${1:parameter})=>{",
			"${2:statement}",
			"}"
		],
		"description": "example: (parameter)=>{ statement}"
	},
	"require": {
		"prefix": "ccreq",
		"body": [
			"${2:moduleName}=require('${1:modulePath}')"
		],
		"description": ""
	},
	"standard function": {
		"prefix": "ccfunction",
		"body": [
			"function(${1:parameter}){",
			"	${2:statement}",
			"};"
		],
		"description": ""
	},
	"mongoose: new Schema": {
		"prefix": "ccnewschema",
		"body": [
			"${1:schemaName} = new Schema({",
			"	${2:statement}",
			"});"
		],
		"description": ""
	},
	"mongoose: property": {
		"prefix": "ccprop",
		"body": [
			"${1:propertyname}: {",
			"	type: ${2:dataType},",
			"	default: ${3:defaultValue}"
			"}"
		],
		"description": ""
	}
}